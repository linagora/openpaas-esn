{
  "webserver": {
    "enabled": true,
    "port": 8080,
    "virtualhosts": []
  },
  "wsserver": {
    "enabled": true,
    "port": 8080
  },
  "log": {
    "file": {
      "enabled": true,
      "filename": "./log/mozilla-updater.log",
      "level": "info",
      "handleExceptions": true,
      "json": false,
      "prettyPrint": true,
      "colorize": false
    },
    "console": {
      "enabled": true,
      "level": "debug",
      "handleExceptions": true,
      "json": false,
      "prettyPrint": true,
      "colorize": true
    }
  },
  "auth": {
    "strategy": "file"
  }
}